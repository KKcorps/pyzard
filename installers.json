{
  "Ubuntu":{
    "axel" : {
      "ppa" : "none"
    },
    "youtube-dl" : {
      "ppa" : "none"
    },
    "git" : {
      "ppa" : "none"
    },
    "vim" : {
      "ppa" : "none"
    },
    "calibre" : {
      "ppa" : "none"
    },
    "google-chrome" : {
      "ppa" : "none",
      "cmds" : {
        "sudo apt-get install libxss1 libappindicator1 libindicator7",
        "wget https://dl.google.com/linux/direct/google-chrome-stable_current_amd64.deb",
        "sudo dpkg -i google-chrome*.deb"
      }
    },
    "node.js" : {
      "ppa" : "none",
      "cmds" : {
        "sudo apt-get install npm",
        "sudo ln -s /usr/bin/nodejs /usr/bin/node"
      }
    },
    "ruby" : {
      "ppa" : "none",
      "cmds" : {
        "curl -sSL https://get.rvm.io | bash -s stable",
        "source /home/"+username+"/.rvm/scripts/rvm"
        "rvm requirements",
        "rvm install 2.1.2"
      },
      "requires" : {
        "curl"
      }
    },
    "jekyll" : {
      "ppa" : "none",
      "cmds" : {
        "gem install jekyll"
      },
      "requires" : {
        "nodejs",
        "ruby"
      }
    },
    "linuxdcpp" : {
      "ppa" : "none"
    },
    "sublime-text-installer" : {
      "ppa" : "ppa:webupd8team/sublime-text-3"
    },
    "vlc" : {
      "ppa" : "none"
    },
    "transmission" : {
      "ppa" : "none"
    },
    "oracle-java8-installer" : {
      "ppa" : "ppa:webupd8team/java"
    },
    "ubuntu-restricted-extras" : {
      "ppa" : "none"
    },
    "ubuntu-tweaks" : {
      "ppa" : "none",
      "cmds" : {
        "wget https://launchpad.net/ubuntu-tweak/0.8.x/0.8.7/+download/ubuntu-tweak_0.8.7-1~trusty2_all.deb",
        "sudo dpkg -i ubuntu-tweak_0.8.7-1~trusty2_all.deb",
        "sudo apt-get install -f"
      }
    },
    "calibre" : {
      "ppa" : "ppa:n-muench/calibre2"
    },
    "gdebi" : {
      "ppa" : "none"
    }
  }
}